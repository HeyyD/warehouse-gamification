{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "semicolon": [true, "always"],
    "no-shadowed-variable": [
      false,
      {
        "class": false,
        "enum": false,
        "function": false,
        "interface": false,
        "namespace": false,
        "typeAlias": false,
        "typeParameter": false
      }
    ],
    "no-console": ["log"]

  }
}
